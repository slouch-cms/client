{
    "name": "slouch-cms/client",
    "description": "A client API for use with the Slouch CMS",
    "autoload": {
        "psr-4": {
            "SlouchCMS\\Client\\": "src"
        }
    },
    "require": {
        "laravel/framework": "^10.0"       
    },
    "extra": {
        "laravel": {
            "providers": [
                "SlouchCMS\\Client\\SlouchCMSServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
